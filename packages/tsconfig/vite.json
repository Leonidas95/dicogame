{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite",
  "extends": "./base.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "allowJs": false,
    "esModuleInterop": false,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["vite/client"]
  },
  "include": ["src"]
}
